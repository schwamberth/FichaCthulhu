<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="br.com.fichacthulhu"
    android:versionCode="1"
    android:versionName="1.0.0">

    <application
        android:name=".FichaCthulhuApp_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity_"
            android:label="Ficha Cthulhu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.InputFicha_"
            android:label="Criar Ficha de Investigador"/>
        <activity android:name=".activity.ListSkillsActivity_"
            android:label="Lista de Perícias"/>
        <activity android:name=".activity.VisualizaInvestigador_"
            android:label="Visualização do Investigador"/>
    </application>

    <dist:module dist:instant="true" />

</manifest>